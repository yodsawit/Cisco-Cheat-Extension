{
    "manifest_version": 3,
    "name": "Cisco Cheat Extension",
    "version": "1.0",
    "description": "Donation pls",
    "author": "arby",
    "permissions": [
        "storage",
        "webRequest",
        "activeTab"
    ],
    "host_permissions": [
        "https://www.netacad.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    }
}
